<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval">
            <corners
                android:topLeftRadius="78dp"
                android:topRightRadius="78dp"
                android:bottomRightRadius="78dp"
                android:bottomLeftRadius="78dp">
            </corners>
            <gradient
                android:startColor="#22aa76"
                android:endColor="#74c947">
            </gradient>
            <padding android:top="12dp"
                android:right="12dp"
                android:left="12dp"
                android:bottom="12dp"/>
        </shape>
    </item>
</layer-list>

